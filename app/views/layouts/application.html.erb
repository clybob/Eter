<!DOCTYPE html>
<html>
<head>
  <title>Eter</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="navbar">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="<%= root_path %>">Eter</a>
        <div class="nav-collapse">
          <ul class="nav">
            <li><%= link_to "Awards", awards_path %></li>
            <li><%= link_to "Badges", badges_path %></li>
            <li><%= link_to "Editorials", editorials_path %></li>
            <li><%= link_to "Events", events_path %></li>
            <li><%= link_to "Points", points_path %></li>
            <li><%= link_to "Users", users_path %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>

<div class="container">
    <div class="row">
            <%= render 'layouts/search' %>
        <div class="span12">
            <%= yield %>
        </div>
    </div>
</div>
</body>
</html>
